<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="company_form_inherit_telenor_sms" model="ir.ui.view">
        <field name="name">res.company.form.inherit.telenor.sms</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="SMS Setting">
                    <group>
                        <group>
                            <field name="msisdn"/>
                            <field name="password" password="True"/>
                            <field name="mask"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
