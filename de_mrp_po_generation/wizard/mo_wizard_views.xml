<odoo>
  <data>
      
      <record id="mo_before_menu_wizard_form" model="ir.ui.view">
            <field name="name">mrp.mo.beforehand.wizard.form</field>
            <field name="model">mrp.mo.beforehand.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="partner_id" />

                        </group>
                        <group>

                        </group>
                    </group>
                
                    <footer>
                        <button string="Add Vendor" name="amend_entries" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="mo_before_menu_wizard_action" model="ir.actions.act_window">
            <field name="name">Create PO from MOs</field>
            <field name="res_model">mrp.mo.beforehand.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
   
      
  </data>
</odoo>