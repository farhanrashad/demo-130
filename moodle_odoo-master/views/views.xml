<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
<!--                    Tree View                   -->
        <record id="exit_request_tree" model="ir.ui.view">
			<field name="name">Moodle.Class.tree</field>
			<field name="model">moodle.config</field>
			<field name="arch" type="xml">
				<tree string="moodle class Tree">
                    <field name="moodle_username" />
				</tree>
			</field>
		</record>

<!--                        Root Action             -->
        <record model="ir.actions.act_window" id="view_courses_form">
            <field name="name">Moodle Class</field>
            <field name="res_model">moodle.config</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Create a Token
                </p>
            </field>
        </record>

    </data>
</odoo>