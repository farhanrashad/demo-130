<odoo>
  <data>
  <record id="paperformat_sale_order_landscape" model="report.paperformat">
  <field name="name">PF Sale Order Landscape</field>
  <field name="default" eval="True"/>
  <field name="format">A4</field>
  <field name="page_height">0</field>
  <field name="page_width">0</field>
  <field name="orientation">Landscape</field>
  <field name="margin_top">10</field>
  <field name="margin_bottom">15</field>
  <field name="margin_left">7</field>
  <field name="margin_right">7</field>
  <field name="header_line" eval="False"/>
  <field name="header_spacing">3</field>
  <field name="dpi">90</field>
 </record> 


  <report
            id="sale_order_report"
            string="Processed Report (PDF)"
            model="sale.order"
            report_type="qweb-pdf"
            file="de_sale_productions_gain_report.report_production_gain_template"
            name="de_sale_productions_gain_report.report_production_gain_template"
            print_report_name="'Processed Report - %s' % (object.name)"
            paperformat="paperformat_sale_order_landscape"


 />
  </data>
</odoo>