<odoo>
  <data>
              
          <record model="ir.ui.view" id="res_townships_tree_view">
            <field name="name">res.townships.tree</field>
            <field name="model">res.townships</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="text_name"/>
                </tree>
            </field>
        </record>
      
      <record model="ir.ui.view" id="res_townships_form_view">
            <field name="name">res.townships.form</field>
            <field name="model">res.townships</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                    
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>                                                                     
                            </group>
                            <group>
                                <field name="text_name"/>
                            </group>
                        </group>
                        
                    </sheet>
                </form>
            </field>
        </record>
      
   <record id="res_townships_menu_action" model="ir.actions.act_window">
        <field name="name">Townships</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.townships</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Add a New Township 
            </p>
        </field>
    </record>
      
   <menuitem id="res_township_menu" name="Townships"
            parent="purchase.menu_purchase_root" action="res_townships_menu_action" sequence="6"/>
    </data>
</odoo>