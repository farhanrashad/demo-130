<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="view_training_batch_form" model="ir.ui.view">
		<field name="name">student.data.form</field>
		<field name="model">student.data</field>
		<field name="arch" type="xml">
			
			<form string="Student Data">
				<sheet>
				<h1>Student Form</h1>
				<group>
				<group>
					
					<field name="age" />
					<field name="address" />
				</group>
				<group>
					<field name="seq_number" />
					<field name="student_name" />
					<field name="father_name" />
					
				</group>
				</group>
				
					
				</sheet>
			</form>
			
		</field>
	</record>
	
	
	<record id="view_training_batch_tree" model="ir.ui.view">
		<field name="name">student.data.tree</field>
		<field name="model">student.data</field>
		<field name="arch" type="xml">
			
			<tree string="Student Data">
					<field name="seq_number" />
					<field name="student_name" />
					<field name="father_name" />
					<field name="age" />
					<field name="address" />
			</tree>
			
		</field>
	</record>


	<record id="view_training_batch_search" model="ir.ui.view">
		<field name="name">student.data.search</field>
		<field name="model">student.data</field>
		<field name="arch" type="xml">
			
			<search string="Student Data">
					<field name="seq_number" />
					<field name="student_name" />
					<field name="father_name" />
			</search>
			
		</field>
	</record>
	


	<record id="action_student_data_view" model="ir.actions.act_window">
		<field name="name">Student Data</field>
		<field name="res_model">student.data</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_mode">tree,form</field>
		<field name="domain">[]</field>
		<field name="context">{}</field>
		<field name="search_view_id" ref="view_training_batch_search" />
		<field name="view_id" ref="view_training_batch_tree" />
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				add student records.....
			</p>
		</field>
	</record>



</odoo>


