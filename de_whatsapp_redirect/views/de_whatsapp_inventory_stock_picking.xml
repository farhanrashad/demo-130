<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="whatsapp_res_partner_stock_picking" model="ir.ui.view">
            <field name="name">stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="before">
                    <div class="oe_button_box" name="button_box">
                        <button name="send_message" string="Whatsapp Message" type="object" class="oe_stat_button" icon="fa-whatsapp"/>
                    </div>
                </xpath>

            </field>
        </record>

        <record id="whatsapp_res_partner_stock_pick" model="ir.ui.view">
            <field name="name">stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <field name="origin" position="after">
                         <field name="mobile_reciept" widget="phone"/>
                </field>
<!--               <xpath expr="/form/sheet/group/group/field[@name='origin']" position="after">-->


<!--                </xpath>-->
            </field>
        </record>

    </data>
</odoo>