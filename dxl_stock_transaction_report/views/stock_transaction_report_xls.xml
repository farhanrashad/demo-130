<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <report
            id="stock_trans_xlsx"
            model="stock.move"
            string="Stock Transaction Report"
            report_type="xlsx"
            name="dxl_stock_transaction_report.stock_trans_xlsx"
            file="dxl_stock_transaction_report.stock_trans_xlsx"
            attachment_use="True"
            menu="False"
        />
        <record id="view_location_form_inherit_reportdxl" model="ir.ui.view">
            <field name="name">stock.location.form.inherit.report</field>
            <field name="model">stock.location</field>
            <field name="inherit_id" ref="stock.view_location_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='removal_strategy_id']" position="after">
                    <field name='wh_id'/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
