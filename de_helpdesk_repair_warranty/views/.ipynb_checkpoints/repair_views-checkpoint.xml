<odoo>
  <data>
        <record id="view_repair_order_form_inherit_helpdesk_repair" model="ir.ui.view">
            <field name="name">repair.form.inherit.heldpesk.repair</field>
            <field name="model">repair.order</field>
            <field name="inherit_id" ref="repair.view_repair_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form[1]/sheet[1]/notebook[1]/page[2]/field[@name='fees_lines']/tree[1]/field[@name='price_subtotal']" position="after">
                    <field name="is_warranty"/>
                </xpath>
            </field>
        </record>

  </data>
</odoo>