<odoo>
  <data>
        <record id="view_sales_warranty_tree_inherit" model="ir.ui.view">
            <field name="name">view.sales.warranty.tree.inherit</field>
            <field name="model">sales.warranty</field>
            <field name="inherit_id" ref="de_sale_warranty.view_sales_warranty_tree"/>
            <field name="arch" type="xml">
                <xpath expr="field[@name='sale_id']" position="after">
                    <field name="repair_id"/>
                </xpath>
            </field>
        </record>

  </data>
</odoo>