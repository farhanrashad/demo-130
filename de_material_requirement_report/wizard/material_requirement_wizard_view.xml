<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="open_wizard_form" model="ir.ui.view">
		<field name="name">open.wizard.form</field>
		<field name="model">material.requirement.wizard</field>
        <field name="arch" type="xml">
			<form string="Material Requirement">
				<sheet>
					<field name="activity_id" readonly="1"/>
				</sheet>

				<footer>
					<button name="print_report" type="object" string="Print" class="oe_highlight"/>
                    <button name="cancel" string="Cancel" class="oe_link" special="cancel"/>
				</footer>

			</form>
		</field>
	</record>

</odoo>