<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="style_view" model="ir.ui.view">
			<field name="name">style.view</field>
			<field name="model">attribute.style</field>
			<field name="arch" type="xml">
				<form>
	                <sheet>
	                	<group>
	                		<group>
	                			<field name="name"/>
	                		</group>
	                	</group>
	                </sheet>
            </form>
			</field>
		</record>
		<record id="style_tree_view" model="ir.ui.view">
			<field name="name">style.tree.view</field>
			<field name="model">attribute.style</field>
			<field name="arch" type="xml">
				<tree string="Style">
	                <field name="name"/>
            	</tree>
			</field>
		</record>
		<record model="ir.actions.act_window" id="action_style">
        	<field name="name">Style</field>
        	<field name="res_model">attribute.style</field>
        	<field name="view_mode">tree,form</field>
    	</record>
    	<menuitem
        name="Style"
        id="menu_style"
        action="action_style"
        parent="extra_attributes.extra_attribute_menu"/>
	</data>
</odoo>