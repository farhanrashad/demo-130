<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <report
            id="inv_sale_xlsx"
            model="pos.order"
            string="POS Excel Report"
            report_type="xlsx"
            name="dxl_inventory_reports.inv_sale_xlsx"
            file="dxl_inventory_reports.inv_sale_xlsx"
            attachment_use="True"
            menu="False"
        />
        <record id="view_location_form_inherit_report" model="ir.ui.view">
            <field name="name">stock.location.form.inherit.report</field>
            <field name="model">stock.location</field>
            <field name="inherit_id" ref="stock.view_location_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='removal_strategy_id']" position="after">
                    <field name='shop_id'/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
