<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.module.category" id="module_category_dysf">
        <field name="name">Resupply Stock Request</field>
   <field name="description">Helps you handle your Basic needs.</field>
        <field name="sequence">10</field>
    </record>

    
    
    <record id="group_dysf_request_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_dysf"/>
    </record>
    <record id="group_dysf_request_manager" model="res.groups">
        <field name="name">Manager</field>
        <!-- <field name="implied_ids" eval="[(4, ref('resupply_stock.group_dysf_request_user'))]"/> -->
        <field name="category_id" ref="module_category_dysf"/>
    </record>
    <record id="resupply_stock_show_user_rule" model="ir.rule">
        <field name="name">Show User Request</field>
        <field name="model_id" ref="resupply_stock.model_resupply_stock"/>
        <field name="domain_force">[('logged_user','=', user.id)]</field>
        <field name="groups" eval="[(4, ref('resupply_stock.group_dysf_request_user'))]"/>
    </record>
    <record id="resupply_stock_show_manager_rule" model="ir.rule">
        <field name="name">Show Manager Request</field>
        <field name="model_id" ref="resupply_stock.model_resupply_stock"/>
        <field name="domain_force">[(1,'=', 1)]</field>
        <field name="groups" eval="[(4, ref('resupply_stock.group_dysf_request_manager'))]"/>
    </record>
</odoo>
