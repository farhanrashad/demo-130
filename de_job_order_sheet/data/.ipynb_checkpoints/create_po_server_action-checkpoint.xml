<?xml version="1.0" ?>

<odoo>

    <data>

        <!-- Sequences for sale.order -->
        <record id="job_material_planning_server_action" model="ir.actions.server">
            <field name="name">Create PO</field>
            <field name="model_id" ref="de_job_order_sheet.model_job_order_sheet_line"/>
            <field name="binding_model_id" ref="de_job_order_sheet.model_job_order_sheet_line"/>            
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.action_generate_purchase_order() </field>
        </record>
        
        <record id="job_order_update_quantity_server_action" model="ir.actions.server">
            <field name="name">Update Quantity</field>
            <field name="model_id" ref="de_job_order_sheet.model_job_order_sheet_line"/>
            <field name="binding_model_id" ref="de_job_order_sheet.model_job_order_sheet_line"/>            
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.action_stock_quantity_update() </field>
        </record>

    </data>
</odoo>