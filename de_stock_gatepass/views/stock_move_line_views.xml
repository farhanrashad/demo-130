<odoo>

	<record id="stock_move_line_tree_view_new" model="ir.ui.view">
		<field name="name">stock.move.line.tree</field>
		<field name="model">stock.move.line</field>
		<field name="arch" type="xml">
			<tree string="Stock Move Line">
				<field name="gatepass_id" />
				<field name="reference" />
				<field name="product_id" />
				<field name="gatepass_qty" />
			</tree>
		</field>
	</record>

	<record id="action_stock_move_adjustment"
		model="ir.actions.act_window">
		<field name="name">Move Lines</field>
		<field name="res_model">stock.move.line</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_mode">tree</field>
		<field name="view_id" ref="stock_move_line_tree_view_new" />
		<field name="help" type="html">
			<p class="oe_view_nocontent_create">
				Click Here To Create New Stock Moves.
			</p>
		</field>
	</record>

	<menuitem id="stock_move_line_menu_id"
			  name="Stock Moves Lines"
			  parent="stock.menu_stock_warehouse_mgmt"
			  action="action_stock_move_adjustment"
			  sequence="31"
	/>

</odoo>