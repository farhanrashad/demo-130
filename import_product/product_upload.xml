<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

     <record id="wizard_import_script_form_view" model="ir.ui.view">
        <field name="name">wizard.import.script.form </field>
        <field name="model">wizard.import.script</field>
        <field name="arch" type="xml">
            <form string="Import Invoice">
                <group>
                    <field name="file" filename="name"/>
                </group>
                <footer>
                    <button name="import_product" string="Import Product" type="object" class="oe_highlight"/>
                </footer>
            </form>
        </field>
    </record>

        <record id="action_wizard_import_script" model="ir.actions.act_window">
            <field name="name">Import Product</field>
            <field name="res_model">wizard.import.script</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    <menuitem
        action="action_wizard_import_script"
        id="menu_action_wizard_import_script"
        parent="stock.menu_stock_inventory_control"
        groups="import_product.group_import_product"
        sequence="150"/>

